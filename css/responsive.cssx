	}
	.footer-top .footer-menu ul li{
		margin: 5px 7px;
	}

	.home-slider{
		margin-bottom: 60px;
	}
	.home-slider .lmt-home-slider-item{
		height: 230px;
	}
	.home-slider .home-slider-nav{
		bottom: -60px;
		height: 60px;
	}

	.documentary-section .feature-white-box,
	.home-feature .feature-white-box{
