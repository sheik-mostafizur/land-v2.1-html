	}
	.site-main .main-header .navbar .navbar-brand{
		width: 140px;
	}
	.header-search{
		max-width: 170px;
	}

	.terms-step-table .table tr td:first-child{
		width: 100px;
	}
	.terms-step-table .table tr td:nth-child(2){
		width: 30%;
	}
	.terms-step-table .table .steps-title{
		margin: 0 30px;
	}
}
